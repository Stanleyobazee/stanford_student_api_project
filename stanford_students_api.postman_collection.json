{
	"info": {
		"_postman_id": "stanford-students-api",
		"name": "Stanford University Students API",
		"description": "API for managing Stanford University Computer Science students",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"variable": [
		{
			"key": "baseUrl",
			"value": "http://localhost:8080",
			"type": "string"
		}
	],
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
